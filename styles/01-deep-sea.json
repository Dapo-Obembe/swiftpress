{
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"version": 3,
	"title": "Deep-Sea",
	"settings": {
		"color": {
			"palette": [
				{
					"color": "#0D1B2A",
					"name": "Base Dark",
					"slug": "base-dark"
				},
				{
					"color": "#1B263B",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#415A77",
					"name": "Accent 1",
					"slug": "accent-1"
				},
				{
					"color": "#778DA9",
					"name": "Accent 2",
					"slug": "accent-2"
				},
				{
					"color": "#E0E1DD",
					"name": "Contrast",
					"slug": "contrast"
				}
			]
		}
	},
	"styles": {
		"color": {
			"background": "var:preset|color|base-dark",
			"text": "var:preset|color|contrast"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var:preset|color|accent-2"
				},
				":hover": {
					"color": {
						"text": "var:preset|color|accent-1"
					}
				}
			},
			"heading": {
				"color": {
					"text": "var:preset|color|contrast"
				}
			},
			"button": {
				"color": {
					"background": "var:preset|color|contrast",
					"text": "var:preset|color|base-dark"
				},
				":hover": {
					"color": {
						"background": "color-mix(in srgb, var(--wp--preset--color--contrast) 85%, transparent)",
						"text": "var:preset|color|base-dark"
					}
				},
				"spacing": {
					"padding": {
						"bottom": "0.6rem",
						"left": "1.6rem",
						"right": "1.6rem",
						"top": "0.6rem"
					}
				}
			}
		},
		"blocks": {
			"core/button": {
				"variations": {
					"outline": {
						"spacing": {
							"padding": {
								"bottom": "0.6rem",
								"left": "1.6rem",
								"right": "1.6rem",
								"top": "0.6rem"
							}
						}
					}
				}
			}
		}
	}
}
